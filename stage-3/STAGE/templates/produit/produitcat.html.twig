{% extends 'backtemplate.html.twig' %}{% block title%} Liste des Produits par Catégorie{% endblock %}{% block body %}{{ form_start(form) }}<div class="form-row align-items-end" >    <div class="col">        {{ form(form) }}    </div>    <div class="col">        <div class="form-group">            <button type="submit" class="btn btn-success">Rechercher</button>        </div>    </div></div>{{ form_end(form) }}{% if produits %}<table id="produits" class="table table-striped">    <thead>    <tr>        <th>Nom</th>        <th>Prix</th>        <th>Quantite</th>        <th>Catégorie</th>        <th>Actions</th>    </tr>    </thead>    <tbody>    {% for produit in produits %}        <tr>            <td>{{ produit.nom }}</td>            <td>{{ produit.prix }}</td>            <td>{{ produit.quantite }}</td>            <td>{{ produit.categorie}}</td>            <td>                <a href="{{ path('produit_index') }}">back to list</a>                <a href="{{ path('produit_edit', {'id': produit.id}) }}">edit</a>            </td>        </tr>    {% endfor %}    </tbody></table>{% else %}    <p>Aucun produits</p>{% endif %}{% endblock %}